<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MAUI_App.MVVM.Views.FirstPage"
             Title="FirstPage">
    <NavigationPage.TitleView>
        <Frame Margin="10"
       Padding="10"
       >
            <SearchBar Placeholder="Search Text here .."/>
        </Frame>
    </NavigationPage.TitleView>
    <StackLayout>
        <Entry Placeholder="Name" x:Name="nametxt"/>
        <Button Text="Second Page" Clicked="Button_Clicked_1" HorizontalOptions="Center" VerticalOptions="Center"/>
        <Button Text="Second Page" Clicked="Button_Clicked"  HorizontalOptions="Center" VerticalOptions="Center"/>

    </StackLayout>
</ContentPage>